<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xcmis.feefax.modules.dao.GlzwDao">
	<select id="findAllList" resultType="Glzw" parameterType="Glzw">
		SELECT
			BILL_NO as billNo
		FROM
			VW_GL_ZWBMKXDC
		WHERE
			BILL_NO=#{billNo}
	</select>
</mapper>